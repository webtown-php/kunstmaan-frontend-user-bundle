{
    "name": "webtown/kunstmaan-frontend-user-bundle",
    "type": "kunstmaan-bundle",
    "description": "Kunstmaan frontend user management bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/webtown-php/kunstmaan-frontend-user-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "gabe@webtown.hu",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "symfony/symfony": "~3.2",
        "kunstmaan/bundles-cms": "4.0.0@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Webtown\\KunstmaanFrontendUserBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webtown\\KunstmaanFrontendUserBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}